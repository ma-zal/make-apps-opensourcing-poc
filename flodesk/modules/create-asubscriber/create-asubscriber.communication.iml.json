{
	"url": "/subscribers",
	"method": "POST",
	"body": {
		"{{...}}": "{{omit(parameters, 'optin_timestamp')}}",
		"optin_timestamp": "{{if(parameters.optin_timestamp, formatDate(parameters.optin_timestamp, 'YYYY-MM-DDTHH:mm:ss.SSS\\Z', 'UTC'))}}"
	},
	"response": {
		"output": {
			"{{...}}": "{{omit(body, 'optin_timestamp', 'created_at')}}",
			"optin_timestamp": "{{parseDate(body.optin_timestamp)}}",
			"created_at": "{{parseDate(body.created_at)}}"
		}
	}
}